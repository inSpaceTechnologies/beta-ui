  "license-check-and-add-config": {
    "folder": ".",
    "license": "header.txt",
    "exact_paths_method": "INCLUDE",
    "exact_paths": [
      "src"
    ],
    "file_type_method": "INCLUDE",
    "file_types": [
      ".js",
      ".vue",
      ".css"
    ],
    "license_formats": {
      "js|ts|css|scss|less|php|as|c|java|cpp|go|cto|acl": {
        "prepend": "/*",
        "append": "*/"
      },
      "html|xml|vue": {
        "prepend": "<!--",
        "append": "-->"
      },
      "gitignore|npmignore|eslintignore|dockerignore|sh|py": {
        "eachLine": {
          "prepend": "# "
        }
      },
      "txt": {}
    },
    "insert_license": true,
    "clear_license": false
  }
